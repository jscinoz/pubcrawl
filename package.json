{
    "name": "pubcrawl",
    "version": "0.1.0",
    "author": "Jack Coulter <jscinoz@jscinoz.so>",
    "description": "Mailing list plugin for Haraka",
    "dependencies": {
        "mongoose": "2.7.x",
        "express": "3.0.x",
        "jade": "0.27.x",
        "Haraka": "2.8.x",
        "less-middleware": "0.1.x",
        "connect-flash": "0.1.x",
        "q": "0.8.x"
    },
    "devDependencies": {
        "grunt": "0.4.x",
        "grunt-contrib-jshint": "0.1.x",
        "grunt-contrib-nodeunit": "0.1.x",
        "grunt-contrib-watch": "0.1.x"
    },
    "engines": {
        "node": "0.6.x"
    },
    "scripts": {
        "postinstall": "haraka -fi haraka > /dev/null 2>&1",
        "start": "haraka -c haraka"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/jscinoz/pubcrawl.git"
    }
}
